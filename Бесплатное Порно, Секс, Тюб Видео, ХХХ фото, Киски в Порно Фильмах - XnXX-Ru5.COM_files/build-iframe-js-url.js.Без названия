(function(exoDynamicParams){(function(){function getSearchObject(){const query=window.location.search;return query?(/^[?#]/.test(query)?query.slice(1):query).split("&").reduce((params,param)=>{let[key,value]=param.split("=");params[key]=value?decodeURIComponent(value.replace(/\+/g," ")):"";return params},{}):{}}const params=getSearchObject();let queryParams='';Object.keys(params).forEach(item=>{if(item!=="idzone"){queryParams+="&"+item+"="+params[item]}});const hostname="https://"+exoDynamicParams.adsDomain;const script=document.createElement('script');script.type="application/javascript";script.src=hostname+'/iframe.js?idzone='+exoDynamicParams.idzone+queryParams;document.body.appendChild(script)})()})({"adsDomain":"a.magsrv.com","idzone":171919})